{
  "name": "cuny-rmp-bot",
  "version": "1.0.0",
  "description": "CUNY Professor Assistant - Queens College Grade Distribution Analysis",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server/index.js",
    "dev:client": "cd client && npm start",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "ingest": "node server/scripts/ingestGrades.js",
    "test-ingest": "node server/scripts/testIngestion.js"
  },
  "keywords": ["cuny", "queens-college", "grade-distribution", "professor-analysis"],
  "author": "CUNY RMP Bot",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "node-cache": "^5.1.2",
    "sqlite3": "^5.1.6",
    "csv-parser": "^3.0.0",
    "openai": "^4.20.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  }
}